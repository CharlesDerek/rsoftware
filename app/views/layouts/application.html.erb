                    );
                    var MentorCategory2 = slug(
                        $(this).find('.mentor-category-inlay:nth-child(2) p').text()
                    );
                    var MentorCategory3 = slug(
                        $(this).find('.mentor-category-inlay:nth-child(3) p').text()
                    );
                    var MentorCategory4 = slug(
                        $(this).find('.mentor-category-inlay:nth-child(4) p').text()
                    );
                    var MentorCategory5 = slug(
                        $(this).find('.mentor-category-inlay:nth-child(5) p').text()
                    );

                    $(this).addClass(MentorCategory1);
                    $(this).addClass(MentorCategory2);
                    $(this).addClass(MentorCategory3);
                    $(this).addClass(MentorCategory4);
                    $(this).addClass(MentorCategory5);

                });

                // Show & Hide Items when Filter Navigation is clicked
                $('.filter-item').click(function () {

                    var LinkCategory = slug($(this).text());

                    $('#filter-list .w-dyn-item')
                        .delay(400)
                        .css('display', 'none');
                    $('.' + LinkCategory)
                        .delay(400)
                        .css('display', 'block');

                    $('.filter-item').removeClass('filter-active');
                    $(this).addClass('filter-active');
                });

                // Show All
                $('.filter-item:first-child').click(function () {

                    $('#filter-list .w-dyn-item')
                        .delay(400)
                        .css('display', 'block');
                });

                // Set Active for All
                $('.filter-item:first-child').addClass('filter-active');

            });
        </script>

        <script>

            $(document).ready(function () {

                if ($(window).width() <= 480) {
                    // do something here

                    var maxHeight = 0;

                    $(".partner-deal-wrapper").each(function () {
                        if ($(this).height() > maxHeight) {
                            maxHeight = $(this).height();
                        }
                    });

                    $(".partner-deal-wrapper").height(maxHeight);
                }

            });
        </script>

        <script>
            function parallax() {

                if (parseInt($(window).width()) > 1365) {
                    var scrolled = $(window).scrollTop();

                    $('.legacy-part1-image3').css('top', -((scrolled * 0.04) - 20) + 'px');
                    $('.legacy-part1-image2').css('top', -((scrolled * 0.06) - 80) + 'px');
                    $('.legacy-part1-column-right').css('top', -((scrolled * -0.02) + 10) + 'px');

                    $('.slide-content').css('top', -((scrolled * 0.04) - 80) + 'px');

                    $('.landing-slide').css('top', -(scrolled * -0.25) + 'px');
                    $('.landing-slide-content').css('top', -(scrolled * 0.25) + 'px');

                    $('.legacy-part3-column-right').css('top', -((scrolled * -0.045)) + 'px');
                    $('.legacy-part3-column-left').css('top', -((scrolled * +0.03)) + 'px');
                    $('.legacy-part3-image3').css('top', -((scrolled * 0.06)) + 'px');
                }
            }

            $(window).scroll(function (e) {
                parallax();
            });
        </script>

        <script>
            $(document).ready(function () {
                var day = $('#invisible-day')
                    .text()
                    .toString();
                var month = $('#invisible-month')
                    .text()
                    .toString();
                var year = $('#invisible-year')
                    .text()
                    .toString();

                var date = year += "/".concat(month += "/".concat(day));

                $("#countdown-days").countdown(date, function (event) {
                    $(this).text(event.strftime('%D'));
                });

                $("#countdown-hours").countdown(date, function (event) {
                    $(this).text(event.strftime('%H'));
                });

                $("#countdown-minutes").countdown(date, function (event) {
                    $(this).text(event.strftime('%M'));
                });

                $("#countdown-seconds").countdown(date, function (event) {
                    $(this).text(event.strftime('%S'));
                });

            });
        </script>
    </body>
</html>
